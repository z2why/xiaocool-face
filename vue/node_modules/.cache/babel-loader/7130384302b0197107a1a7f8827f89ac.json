{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"tabAudit\",\n\n  data() {\n    return {\n      getData: [],\n      pageNum: 1,\n      pageSize: 8,\n      mname: '',\n      uname: '',\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n\n  created() {\n    //请求分页查询数据\n    // this.user=  JSON.parse(localStorage.getItem(\"user\"))\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.request.get(\"/AboutMeeting/findMember\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          createUser: this.user.id\n        }\n      }).then(res => {\n        this.getData = res.data;\n      });\n    },\n\n    check(udid, participateStatus, mname, userId, meetingId, uname) {\n      this.$confirm('是否通过 \"' + uname + '\" 关于《' + mname + '》 的补签申请?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        const from1 = {\n          id: udid,\n          participateStatus: 1,\n          meetingId: meetingId,\n          userId: userId\n        }; //修改\n\n        this.request.post('/AboutMeeting', from1).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"补签成功\");\n            this.load();\n          } else {\n            this.$message.error(\"补签失败\");\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消提交'\n        });\n        this.load();\n      });\n    },\n\n    checkout(udid, participateStatus, mname, userId, meetingId, uname) {\n      this.$confirm('是否拒绝 \"' + uname + '\" 关于《' + mname + '》 的补签申请?', '提示', {\n