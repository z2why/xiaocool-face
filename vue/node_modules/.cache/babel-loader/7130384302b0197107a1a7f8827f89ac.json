urcesContent":["<template>\r\n<!--补签审核-->\r\n<div CLASS=\"MeetingMember\">\r\n    <el-table :data=\"getData\"   style=\"margin-bottom: 10px\">\r\n\r\n      <el-table-column\r\n          prop=\"code\" label=\"会议号\" width=\"150\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"mname\" label=\"会议名称\" width=\"300\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <a @click=\"MeetingDetail(scope.row)\" style=\"color: #409eff;cursor:pointer\">{{scope.row.mname}}</a>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"userId\" label=\"用户编号\" width=\"150\" align=\"center\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"uname\" label=\"用户姓名\" width=\"180\" align=\"center\">\r\n      </el-table-column>\r\n\r\n\r\n      <el-table-column\r\n          prop=\"participateStatus\" label=\"签到情况\" width=\"230\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag size=\"small\"  v-if=\"scope.row.participateStatus === 0\"  type=\"info \" >未签到</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.participateStatus === 1\"  >已签到</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.participateStatus === 2\" type=\"warning\" effect=\"dark\">迟到</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.participateStatus === 3\" type=\"warning\" effect=\"dark\">早退</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          prop=\"stage\" label=\"会议状态\" width=\"220\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag size=\"small\"  v-if=\"scope.row.stage === 0\"  >未开启</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.stage === 1\"   >进行中</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.stage === 2\" type=\"info\" effect=\"dark\">已结束</el-tag>\r\n          <el-tag size=\"small\" v-else-if=\"scope.row.stage === 3\" type=\"warning\" effect=\"dark\">暂缓中</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"操作\"\r\n          prop=\"icon\"\r\n          align=\"center\">\r\n\r\n        <template slot-scope=\"scope\" >\r\n          <el-button icon=\"el-icon-check\" type=\"success\" v-model=\"scope.row.participateStatus\" active-color=\"#13ce66\"\r\n                     size=\"medium\" inactive-color=\"#ccc\" @click=\"check(scope.row.udid,scope.row.participateStatus,scope.row.mname,scope.row.userId,scope.row.meetingId,scope.row.uname)\" plain>通过\r\n          </el-button>\r\n          <el-button icon=\"el-icon-close\" type=\"warning\" v-model=\"scope.row.participateStatus\" active-color=\"#13ce66\"\r\n                     size=\"medium\" inactive-color=\"#ccc\" @click=\"checkout(scope.row.udid,scope.row.participateStatus,scope.row.mname,scope.row.userId,scope.row.meetingId,scope.row.uname)\" plain>不通过\r\n          </el-button>\r\n  <!--          <el-button icon=\"el-icon-close\" type=\"danger\" v-model=\"scope.row.participateStatus\" active-color=\"#13ce66\"-->\r\n  <!--                     size=\"medium\"    inactive-color=\"#ccc\" @click=\"checkout(scope.row.udid,scope.row.participateStatus)\" plain>不通过-->\r\n  <!--          </el-button>-->\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <div style=\"margin-top: 10px;position: relative;left: 35%\">\r\n    </div>\r\n\r\n\r\n</div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"tabAudit\",\r\n  data(){\r\n    return{\r\n      getData:[],\r\n      pageNum: 1,\r\n      pageSize: 8,\r\n      mname:'',\r\n      uname:'',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n    }\r\n  },\r\n\r\n  created() {\r\n    //请求分页查询数据\r\n    // this.user=  JSON.parse(localStorage.getItem(\"user\"))\r\n    this.load()\r\n  },\r\n  methods: {\r\n\r\n    load() {\r\n      this.request.get(\"/AboutMeeting/findMember\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          createUser:this.user.id\r\n        }\r\n\r\n      }).then(res => {\r\n            this.getData = res.data\r\n          }\r\n      )\r\n    },\r\n    check(udid,participateStatus,mname,userId,meetingId,uname) {\r\n      this.$confirm('是否通过 \"'+uname+'\" 关于《'+ mname + '》 的补签申请?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        const from1 = {\r\n          id: udid,\r\n          participateStatus: 1,\r\n          meetingId:meetingId,\r\n          userId: userId\r\n\r\n        }\r\n        //修改\r\n        this.request.post('/AboutMeeting', from1)\r\n            .then(res => {\r\n              if (res.code === '200') {\r\n                this.$message.success(\"补签成功\")\r\n                this.load()\r\n              } else {\r\n                this.$message.error(\"补签失败\")\r\n              }\r\n            })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消提交'\r\n        });\r\n        this.load() })\r\n    },\r\n\r\n    checkout(udid,participateStatus,mname,userId,meetingId,uname) {\r\n      this.$confirm('是否拒绝 \"'+uname+'\" 关于《'+ mname + '》 的补签申请?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        const from1 = {\r\n          id: udid,\r\n          meetingId:meetingId,\r\n          userId: userId,\r\n          supplementarySign:0\r\n        }\r\n        //修改\r\n        this.request.post('/AboutMeeting', from1)\r\n            .then(res => {\r\n              if (res.code === '200') {\r\n                  this.$message.warning(\"操作成功\")\r\n                this.load()\r\n              } else {\r\n                this.$message.error(\"操作失败\")\r\n                this.load()\r\n              }\r\n            })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消提交'\r\n        });\r\n        this.load() })\r\n    },\r\n\r\n\r\n  }}\r\n</script>\r\n\r\n<style scoped>\r\n*{\r\nfont-size: larger;\r\n}\r\n.el-tag--small {\r\n  height: 33px;\r\n  padding: 4px 13px;\r\n  line-height: 22px;\r\n  font-size: initial;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}