{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue'; // editor.create()\n\nexport default {\n  name: \"User\",\n  components: {\n    Editor,\n    Toolbar\n  },\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      currentPage: 1,\n      total: 0,\n      tableData: [],\n      search: '',\n      pageSize: 3,\n      headerBg: 'headerBg',\n      username: '',\n      multipleSelection: [],\n      delBtlStatu: true,\n      editor: null,\n      html: '<p>hello</p>',\n      toolbarConfig: {},\n      editorConfig: {\n        placeholder: '请输入内容...'\n      },\n      mode: 'default' // or 'simple'\n\n    };\n  },\n\n  created() {\n    //请求分页查询数据\n    this.load();\n  },\n\n  methods: {\n    onCreated(editor) {\n      this.editor = Object.seal(editor); // 一定要用 Object.seal() ，否则会报错\n    },\n\n    handleAdd() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    handleDelete(id) {\n      this.request.delete(\"/service/audit/user/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); // [{},{}] =>[1,2,3]\n\n      this.request.post(\"/service/audit/user/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n\n    handleExcelImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    },\n\n    save() {\n      this.request.post(\"/service/audit/user\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功\");\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n\n    load() {\n      this.request.get(\"/service/audit/user/page\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          username: this.username\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    //多选\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n      this.delBtlStatu = val.length == 0;\n    },\n\n    reset() {\n      this.username = \"\";\n      this.load();\n    },\n\n    handleEdit(row) {\n      this.form = row;\n      this.dialogVisible = true;\n    },\n\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      this.currentPage = pageNum;\n      this.load();\n    },\n\n    //上传头像\n    filesUploadSuccess(res) {\n      console.log(res);\n      this.form.avatar = res;\n      this.load();\n    },\n\n    //上传人脸识别照片\n    filesUploadSuccess1(res) {\n      console.log(res);\n      this.form.faceImage = res;\n      this.load();\n    },\n\n    exp() {\n      window.open(this.$baseUrl + \"/service/audit/user/export\");\n    }\n\n  },\n\n  // mounted() {\n  //   // 模拟 ajax 请求，异步渲染编辑器\n  //   setTimeout(() => {\n  //     this.html = '<p>模拟 Ajax 异步设置内容 HTML</p>'\n  //   }, 1500)\n  // },\n  beforeDestroy() {\n    const editor = this.editor;\n    if (editor == null) return;\n    editor.destroy(); // 组件销毁时，及时销毁编辑器\n  }\n\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwQA,6D,CACA;;AAEA;EACAA,YADA;EAEAC;IAAAC;IAAAC;EAAA,CAFA;;EAIAC;IAEA;MACAC,QADA;MAEAC,oBAFA;MAGAC,cAHA;MAIAC,QAJA;MAKAC,aALA;MAMAC,UANA;MAOAC,WAPA;MAQAC,oBARA;MASAC,YATA;MAUAC,qBAVA;MAWAC,iBAXA;MAYAC,YAZA;MAaAC,oBAbA;MAcAC,iBAdA;MAeAC;QAAAC;MAAA,CAfA;MAgBAC,eAhBA,CAgBA;;IAhBA;EAkBA,CAxBA;;EAyBAC;IACA;IACA;EACA,CA5BA;;EA6BAC;IACAC;MACA,kCADA,CACA;IACA,CAHA;;IAIAC;MACA;MACA;IACA,CAPA;;IAQAC;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MAEA,CARA;IAUA,CAnBA;;IAoBAC;MACA,gDADA,CACA;;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MAEA,CARA;IAWA,CAjCA;;IAkCAC;MACA;MACA;IACA,CArCA;;IAsCAC;MACA;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MAEA,CATA;IAUA,CAjDA;;IAkDAC;MACA;QACAC;UACAC,yBADA;UAEArB,uBAFA;UAGAE;QAHA;MADA,GAOAoB,IAPA,CAOAC;QACA;QACA;MACA,CAVA;IAYA,CA/DA;;IAiEA;IACAC;MACAC;MACA;MACA;IACA,CAtEA;;IAuEAC;MACA;MACA;IACA,CA1EA;;IA6EAC;MACA;MACA;IACA,CAhFA;;IAiFAC;MACAH;MACA;MACA;IACA,CArFA;;IAsFAI;MACA;MACA;IACA,CAzFA;;IA2FA;IACAC;MACAL;MACA;MACA;IACA,CAhGA;;IAkGA;IACAM;MACAN;MACA;MACA;IACA,CAvGA;;IAwGAO;MACAC;IACA;;EA1GA,CA7BA;;EA2IA;EACA;EACA;EACA;EACA;EACA;EACAC;IACA;IACA;IACA7B,iBAHA,CAGA;EACA;;AArJA","names":["name","components","Editor","Toolbar","data","form","dialogVisible","currentPage","total","tableData","search","pageSize","headerBg","username","multipleSelection","delBtlStatu","editor","html","toolbarConfig","editorConfig","placeholder","mode","created","methods","onCreated","handleAdd","handleDelete","delBatch","handleExcelImportSuccess","save","load","params","pageNum","then","res","handleSelectionChange","console","reset","handleEdit","handleSizeChange","handleCurrentChange","filesUploadSuccess","filesUploadSuccess1","exp","window","beforeDestroy"],"sourceRoot":"src/views","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding: 10px;\"  >\r\n    <div id=\"biaotou\">\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\" style=\"font-size: larger\">\r\n        <el-breadcrumb-item :to=\"{ path: '/AddMeeting' }\"><i class=\"el-icon-s-home\"></i>首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item>用户管理</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n      <el-divider></el-divider>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0;padding: 10px 0;display: flex;flex-direction: row;justify-content: space-between; \" >\r\n      <div style=\"display: flex;gap: 3px;\" >\r\n        <el-button  @click=\"handleAdd\"  icon=\"el-icon-circle-plus-outline\">添加</el-button>\r\n        <el-upload  :action='$baseUrl+\"/service/audit/user/import\"'  :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button   icon=\"el-icon-bottom\" >导入</el-button>\r\n        </el-upload>\r\n        <el-button  icon=\"el-icon-top\" @click=\"exp\">导出</el-button>\r\n\r\n\r\n        <el-popconfirm\r\n            title=\"确定批量删除吗？\" confirm-button-text='确定' cancel-button-type=\"success\" @confirm=\"delBatch\" icon=\"el-icon-warning\" icon-color=\"red\">\r\n          <el-button slot=\"reference\"\r\n                     type=\"danger\" style=\"margin-left: 20px\" icon=\"el-icon-remove-outline\" :disabled=\"delBtlStatu\" >批量删除</el-button>\r\n        </el-popconfirm>\r\n    </div>\r\n\r\n      <div style=\"display: flex\" >\r\n\r\n        <el-input\r\n            style=\"width: 180px;margin-top: 1px;margin-right: 10px\"\r\n            placeholder=\"请输入用户名\"  v-model=\"username\" >\r\n          <i slot=\"suffix\" class=\"el-input__icon el-icon-search\"></i>\r\n        </el-input>\r\n\r\n        <el-button type=\"primary\"   @click=\"load\" plain>搜索</el-button>\r\n        <el-button style=\"margin-left: 5px;\" type=\"warning\"   @click=\"reset\" plain>重置</el-button>\r\n      </div>\r\n\r\n\r\n\r\n\r\n    </div>\r\n    <el-table\r\n        ref=\"multipleTable\"\r\n        :data=\"tableData\"\r\n        border\r\n        stripe\r\n        style=\"width: 100%\"\r\n        tooltip-effect=\"dark\"\r\n        @selection-change=\"handleSelectionChange\"\r\n        :header-cell-class-name=\"headerBg\"\r\n\r\n    >\r\n      <el-table-column\r\n          type=\"selection\"\r\n          width=\"50\"\r\n          align=\"center\"\r\n      >\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"编号\"\r\n          prop=\"id\"\r\n          width=\"90\"\r\n          align=\"center\"\r\n          sortable\r\n      >\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"姓名\"\r\n          prop=\"name\"\r\n          align=\"center\"\r\n          width=\"100\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          align=\"center\"\r\n          width=\"160\"\r\n          label=\"人脸识别照片\">\r\n        <template #default=\"scope\">\r\n          <div class=\"demo-image__preview\">\r\n            <el-image\r\n                style=\"width: 100px; height: 100px\"\r\n                :src=\"scope.row.faceImage\"\r\n                :preview-src-list=\"[scope.row.faceImage]\">\r\n            </el-image>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"用户名\"\r\n          prop=\"username\"\r\n      width=\"160\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          label=\"密码\"\r\n          prop=\"password\"\r\n          width=\"160\">\r\n      </el-table-column>\r\n\r\n      <el-table-column\r\n          width=\"150\"\r\n          align=\"center\"\r\n\r\n          label=\"头像\">\r\n        <template #default=\"scope\">\r\n          <div class=\"demo-image__preview\">\r\n            <el-image\r\n                style=\"width: 100px; height: 100px\"\r\n                :src=\"scope.row.avatar\"\r\n                :preview-src-list=\"[scope.row.avatar]\">\r\n            </el-image>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n\r\n\r\n\r\n      <el-table-column\r\n          prop=\"email\"\r\n          width=\"230\"\r\n          label=\"邮箱\">\r\n      </el-table-column>\r\n\r\n\r\n      <el-table-column\r\n          prop=\"sex\"\r\n          label=\"性别\"\r\n          width=\"80\"\r\n      align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-tag size=\"large\" v-if=\"scope.row.sex === 0\" >女</el-tag>\r\n          <el-tag size=\"large\" v-else-if=\"scope.row.sex === 1\">男</el-tag>\r\n          <el-tag size=\"large\" v-else-if=\"scope.row.sex === 2\">其他</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n<!--            <el-table-column-->\r\n<!--          label=\"签到时间\"-->\r\n<!--          prop=\"timeopen\">-->\r\n<!--      </el-table-column>-->\r\n\r\n<!--      <el-table-column-->\r\n<!--          label=\"签退时间\"-->\r\n<!--          prop=\"timeout\">-->\r\n<!--      </el-table-column>-->\r\n\r\n\r\n      <el-table-column\r\n          fixed=\"right\"\r\n          label=\"操作\"\r\n          width=\"240\"\r\n          align=\"center\"\r\n      >\r\n        <template slot-scope=\"scope\">\r\n          <el-button icon=\"el-icon-edit\"\r\n                     @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-popconfirm\r\n              title=\"确定删除吗？\" confirm-button-text='确定' cancel-button-type=\"success\" @confirm=\"handleDelete(scope.row.id)\" icon=\"el-icon-warning\" icon-color=\"red\">\r\n            <el-button slot=\"reference\"\r\n                       type=\"danger\" icon=\"el-icon-remove-outline\" >删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0;left: 40%; position: absolute;\">\r\n      <el-pagination\r\n\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[3]\"\r\n          :page-size=\"pageSize\"\r\n          background\r\n          layout=\"total, sizes, prev, pager,\r\n          next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n      <el-dialog\r\n          title=\"用户信息\"\r\n          :visible.sync=\"dialogVisible\"  width=\"30%\"  >\r\n        <el-form :model=\"form\" label-width=\"80px\">\r\n\r\n<!--          <el-form-item label=\"编号:\">-->\r\n<!--            <el-input v-model=\"form.id\" readonly style=\"width: 75%\"></el-input>-->\r\n<!--          </el-form-item>-->\r\n\r\n          <el-form-item label=\"头像:\">\r\n<!--            <el-upload  :action='$baseUrl+\"/service/cos/upload\"'-->\r\n\r\n<!--                        :on-success=\"filesUploadSuccess\" style=\"width: 75%\">-->\r\n<!--              <el-button type=\"primary\" icon=\"el-icon-user\" plain>点击上传<i class=\"el-icon-upload el-icon&#45;&#45;right\"></i></el-button>-->\r\n<!--            </el-upload>-->\r\n            <el-upload\r\n                class=\"upload-demo\"\r\n                action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n                :on-preview=\"handlePreview\"\r\n                :on-remove=\"handleRemove\"\r\n                :file-list=\"fileList\"\r\n                list-type=\"picture\">\r\n              <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n              <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\r\n            </el-upload>\r\n            <div class=\"upload-btn\">\r\n              <el-button type=\"primary\" @click=\"submitUpload\" :loading=\"loading\">上 传</el-button>\r\n            </div>\r\n\r\n\r\n          </el-form-item>\r\n\r\n\r\n\r\n          <el-form-item label=\"姓名:\">\r\n            <el-input v-model=\"form.name\" style=\"width: 75%\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"性别:\"  style=\"margin-left: -20px;\" prop=\"sex\" label-width=\"100px\">\r\n            <el-radio-group v-model=\"form.sex\">\r\n              <el-radio :label=\"1\">男</el-radio>\r\n              <el-radio :label=\"0\">女</el-radio>\r\n              <el-radio :label=\"2\">其他</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n\r\n<!--          <el-form-item label=\"照片:\">-->\r\n<!--            <el-upload-->\r\n<!--                class=\"avatar-uploader\"-->\r\n<!--                :action='$baseUrl+\"/file/upload\"'-->\r\n<!--                :show-file-list=\"false\"-->\r\n<!--                :on-success=\"filesUploadSuccess1\">-->\r\n<!--              <img v-if=\"form.faceImage\" :src=\"form.faceImage\" class=\"avatar\">-->\r\n<!--              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>-->\r\n<!--            </el-upload>-->\r\n<!--          </el-form-item>-->\r\n\r\n\r\n\r\n          <el-form-item label=\"用户名:\">\r\n            <el-input v-model=\"form.username\" style=\"width: 75%\"></el-input>\r\n          </el-form-item>\r\n\r\n          <el-form-item label=\"密码:\">\r\n            <el-input v-model=\"form.password\" style=\"width: 75%\"></el-input>\r\n          </el-form-item>\r\n\r\n\r\n        </el-form>\r\n\r\n        <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n          </span>\r\n        </template>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue'\r\n// editor.create()\r\n\r\nexport default {\r\n  name: \"User\",\r\n  components: { Editor, Toolbar },\r\n\r\n  data(){\r\n\r\n    return{\r\n      form:{},\r\n      dialogVisible:false,\r\n      currentPage:1,\r\n      total:0,\r\n      tableData:[],\r\n      search: '',\r\n      pageSize:3,\r\n      headerBg: 'headerBg',\r\n      username: '',\r\n      multipleSelection:[],\r\n      delBtlStatu: true,\r\n      editor: null,\r\n      html: '<p>hello</p>',\r\n      toolbarConfig: { },\r\n      editorConfig: { placeholder: '请输入内容...' },\r\n      mode: 'default', // or 'simple'\r\n    }\r\n  },\r\n  created() {\r\n    //请求分页查询数据\r\n    this.load()\r\n  },\r\n  methods:{\r\n    onCreated(editor) {\r\n      this.editor = Object.seal(editor) // 一定要用 Object.seal() ，否则会报错\r\n    },\r\n    handleAdd(){\r\n      this.dialogVisible = true;\r\n      this.form = {}\r\n    },\r\n    handleDelete(id){\r\n      this.request.delete(\"/service/audit/user/\"+id).then(res => {\r\n        if(res.code === '200'){\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n\r\n      })\r\n\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v=> v.id) // [{},{}] =>[1,2,3]\r\n      this.request.post(\"/service/audit/user/del/batch\",ids).then(res => {\r\n        if(res.code === '200'){\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n\r\n      })\r\n\r\n\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    },\r\n    save(){\r\n      this.request.post(\"/service/audit/user\", this.form).then(res => {\r\n        if(res.code === '200'){\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogVisible =false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n\r\n      })\r\n    },\r\n    load(){\r\n      this.request.get(\"/service/audit/user/page\",{\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          username : this.username\r\n\r\n        }\r\n      }).then(res => {\r\n            this.tableData = res.data.records\r\n            this.total = res.data.total\r\n          }\r\n      )\r\n    },\r\n\r\n    //多选\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection=val\r\n      this.delBtlStatu = val.length == 0\r\n    },\r\n    reset(){\r\n      this.username=\"\"\r\n      this.load()\r\n    },\r\n\r\n\r\n    handleEdit(row){\r\n      this.form = row\r\n      this.dialogVisible = true\r\n    },\r\n    handleSizeChange(pageSize){\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n\r\n    //上传头像\r\n    filesUploadSuccess(res){\r\n      console.log(res);\r\n      this.form.avatar = res;\r\n      this.load();\r\n    },\r\n\r\n    //上传人脸识别照片\r\n    filesUploadSuccess1(res){\r\n      console.log(res);\r\n      this.form.faceImage = res;\r\n      this.load();\r\n    },\r\n    exp(){\r\n      window.open(this.$baseUrl+\"/service/audit/user/export\")\r\n    }\r\n\r\n\r\n  },\r\n  // mounted() {\r\n  //   // 模拟 ajax 请求，异步渲染编辑器\r\n  //   setTimeout(() => {\r\n  //     this.html = '<p>模拟 Ajax 异步设置内容 HTML</p>'\r\n  //   }, 1500)\r\n  // },\r\n    beforeDestroy() {\r\n    const editor = this.editor\r\n    if (editor == null) return\r\n    editor.destroy() // 组件销毁时，及时销毁编辑器\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.el-tag{\r\n    font-size: 18px;\r\n}\r\n::v-deep .el-divider--horizontal{\r\n  margin-bottom: 0;\r\n}\r\n::v-deep .avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n::v-deep .avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n::v-deep .avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n::v-deep .avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n<style>\r\n*{\r\n  font-family: 微软雅黑;\r\n  font-size: 16px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}