ndex=\"2-2\" @click=\"$router.push('/userinfo/ApplyMeeting')\">会议审核历史</el-menu-item>\r\n          <el-menu-item index=\"2-3\" @click=\"$router.push('/userinfo/MeetingList')\">会议列表</el-menu-item>\r\n\r\n        </el-submenu>\r\n        <el-menu-item index=\"3\" @click=\"$router.push('/userinfo')\" style=\"border-left: ridge;  border-left-width: 1px;padding-right: 30px\">用户中心</el-menu-item>\r\n      </el-menu>\r\n    </div>\r\n      <div v-if=\"user===null\">\r\n        <el-row>\r\n          <el-button type=\"primary\" style=\"padding: 20px 13px;\" @click=\"$router.push('/')\" plain circle>登录</el-button>\r\n        </el-row>\r\n      </div>\r\n      <div class=\"user\" v-if=\"user!==null  \">\r\n      <el-dropdown style=\"position: relative;top: -34px;right: 7px;\">\r\n        <div class=\"el-dropdown-link\">\r\n          <img :src=\"user.avatar\" alt=\"\" style=\"  width: 53px; border-radius: 50%;   position: relative;  top: 35px; \">\r\n        </div>\r\n        <el-dropdown-menu slot=\"dropdown\" >\r\n          <template slot-scope=\"scope\">\r\n            <div @click=\"$router.push('/userinfo')\">\r\n              <el-dropdown-item>  {{user.name}} </el-dropdown-item></div>\r\n            <div @click=\"logout()\"> <el-dropdown-item divided>退出登录</el-dropdown-item></div>\r\n          </template>\r\n        </el-dropdown-menu>\r\n      </el-dropdown>\r\n    </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Head\",\r\n  data() {\r\n    return {\r\n      index: '1',\r\n      user:{}\r\n\r\n    };\r\n  },\r\n  created() {\r\n    /*获取当前用户信息*/\r\n    this.user=JSON.parse(localStorage.getItem(\"user\"))\r\n\r\n    //this.getMeetingPageOfMe();\r\n  },\r\n\r\n  methods: {\r\n    logout() {\r\n      this.$store.commit(\"logout\")\r\n      this.$message.success(\"退出成功\")\r\n      var storage = window.localStorage;\r\n      storage.clear()\r\n      this.$router.push('/')\r\n    }\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>