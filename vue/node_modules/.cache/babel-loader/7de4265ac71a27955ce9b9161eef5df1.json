{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: 'Login_Register',\n\n  data() {\n    const validateEmailOrPhone = (rule, value, callback) => {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      const phoneRegex = /^1[3-9]\\d{9}$/; // 假设中国的手机号码格式\n\n      if (!value) {\n        return callback(new Error('请输入邮箱或者手机号码'));\n      } else if (!emailRegex.test(value) && !phoneRegex.test(value)) {\n        return callback(new Error('请输入正确邮箱或者手机号码'));\n      } else {\n        callback();\n      }\n    };\n\n    return {\n      user: {},\n      user1: {},\n      userRegister: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }, {\n          min: 5,\n          max: 8,\n          message: '长度在 5 到 10 个字符',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '长度在 6 到 15 个字符',\n          trigger: 'blur'\n        }],\n        codee: [{\n          required: true,\n          message: '请输入验证码',\n          trigger: 'blur'\n        }, {\n          min: 4,\n          max: 4,\n          message: '验证码错误',\n          trigger: 'blur'\n        }]\n      },\n      rulesCaptcha: {\n        userNumber: [// 手机号或者邮箱\n        {\n          validator: validateEmailOrPhone,\n          trigger: 'blur'\n        }],\n        captcha: [{\n          required: true,\n          message: '请输入验证码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 6,\n          message: '验证码错误',\n          trigger: 'blur'\n        }]\n      },\n      rulesRegister: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 8,\n          message: '长度在 5 到 8 个字符',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '长度在 6 到 15 个字符',\n          trigger: 'blur'\n        }],\n        confirmPassword: [{\n          required: true,\n          message: '请再次输入密码',\n          trigger: 'blur'\n        }, {\n          min: 6,\n          max: 15,\n          message: '长度在 6 到 15 个字符',\n          trigger: 'blur'\n        }]\n      },\n      isLogin: false,\n      loginForm: {\n        email: '',\n        password: ''\n      },\n      registerForm: {\n        name: '',\n        email: '',\n        password: ''\n      },\n      loginByCode: false\n    };\n  },\n\n  created() {\n    this.enterSearch(); // 清除状态保持\n    // 状态保持清除后刷新页面\n  },\n\n  methods: {\n    uploadImage() {\n      document.getElementById(\"codeImg\").src = this.$baseUrl + \"/user/getCode?time=\" + new Date().getTime();\n    },\n\n    login() {\n      this.$refs['userForm'].validate(valid => {\n        if (valid) {\n          // 表单校验合法\n          this.request.post(\"/user/login\", this.user).then(res => {\n            if (res.code === '200') {\n              localStorage.setItem(\"user\", JSON.stringify(res.data)); // 存储用户信息到浏览器\n              // localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // 存储用户信息到浏览器\n              // 动态设置当前用户的路由\n              // setRoutes()\n\n              this.$message.success(\"登录成功\");\n\n              if (res.data.role === 'ROLE_ADMIN') {\n                this.$router.push(\"/Admin/user\");\n              } else if (res.data.role === 'ROLE_USER') {\n                this.$router.push(\"/AddMeeting\");\n              } else {\n                this.$router.push(\"/userinfo\");\n              }\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n\n    LoginGitee() {\n      this.request.get(\"uaa/oauth2/gitee\").then(res => {\n        if (res.code === 200) {\n          \"登陆成功\";\n        }\n      });\n    },\n\n    register() {\n      this.$refs['userRegisterForm'].validate(valid => {\n        if (valid) {\n          // 表单校验合法\n          if (this.userRegister.password !== this.userRegister.confirmPassword) {\n            this.$message.error(\"两次输入的密码不一致\");\n            return false;\n          }\n\n          this.request.post(\"/user/register\", this.userRegister).then(res => {\n            if (res.code === '200') {\n              this.$message.success(\"注册成功\");\n              this.isLogin = !this.isLogin;\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        }\n      });\n    },\n\n    enterSearch() {\n      document.onkeydown = e => {\n        //13表⽰回车键，baseURI是当前页⾯的地址，为了更严谨，也可以加别的，可以打印e看⼀下\n        if (e.keyCode === 13 && e.target.baseURI.match()) {\n          //回车后执⾏搜索⽅法\n          this.login();\n        }\n      };\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoIA;EACAA,sBADA;;EAEAC;IACA;MACA;MACA,mCAFA,CAEA;;MAEA;QACA;MACA,CAFA,MAEA;QACA;MACA,CAFA,MAEA;QACAC;MACA;IACA,CAXA;;IAYA;MACAC,QADA;MAEAC,SAFA;MAGAC,gBAHA;MAIAC;QACAC,WACA;UAAAC;UAAAC;UAAAC;QAAA,CADA,EAEA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,CAFA,CADA;QAKAG,WACA;UAAAL;UAAAC;UAAAC;QAAA,CADA,EAEA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,CAFA,CALA;QAS