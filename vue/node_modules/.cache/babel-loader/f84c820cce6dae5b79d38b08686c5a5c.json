this.signList[this.signList.length - 1].username;\n            console.log(this.signName, \"名字\");\n            this.request.get(\"/AboutMeeting/AllSign\", {\n              params: {\n                name: this.signName,\n                code: this.code\n              }\n            }).then(res => {\n              console.log(res);\n              this.signInfo = res.data;\n              this.load();\n            });\n          }\n        }\n      });\n    },\n\n    load() {\n      this.request.get(\"/AboutMeeting/findMember\", {\n        params: {\n          pageNum: 1,\n          pageSize: 999,\n          code: this.code\n        }\n      }).then(res => {\n        this.meetingList = res.data.records;\n        this.request.get(\"/AboutMeeting/SignNum\", {\n          params: {\n            meetingId: this.meetingList[0].meetingId\n          }\n        }).then(res => {\n          this.signNum = res.data.allcount - res.data.signcount;\n          this.allNum = res.data.allcount;\n          this.rate = Number(this.signNum / this.allNum * 100).toFixed(2);\n        });\n      });\n    },\n\n    errorHandler() {\n      return true;\n    },\n\n    click() {\n      if (!screenfull.isEnabled) {\n        this.$message({\n          message: '你的浏览器不支持全屏',\n          type: 'warning'\n        });\n        return false;\n      }\n\n      screenfull.toggle();\n    },\n\n    change() {\n      this.isFullscreen = screenfull.isFullscreen;\n    },\n\n    init() {\n      if (screenfull.isEnabled) {\n        screenfull.on('change', this.change);\n      }\n    },\n\n    destroy() {\n      if (screenfull.isEnabled) {\n        screenfull.off('change', this.change);\n      }\n    },\n\n    handleExcelImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    },\n\n    close() {\n      this.meeting.stage = 2;\n      this.$confirm('将于现在关闭【' + this.meeting.name + '】, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'error'\n      }).then(() => {\n        //修改\n        this.request.post('/meeting/MyMeeting', this.meeting).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"成功结束会议!\");\n            this.$router.go(-1);\n          } else {\n            this.$message.error(\"失败!\");\n          }\n        });\n      });\n    } //\n\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwJA;AACA;AACA;AACA;EACAA,eADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,eADA;MAEAC,mBAFA;MAEA;MACAC,YAHA;MAIAC,YAJA;MAKAC,aALA;MAMAC,gBANA;MAOAC,gBAPA;MAQAC,YARA;MASAC,UATA;MAUAC,WAVA;MAWAC,QAXA;MAYAC;IAZA;EAcA,CApBA;;EAqBAC;IACA;IACA;IACA;IACA;IACA;EACA,CA3BA;;EA4BAC;IACA,YADA,CAEA;IACA;IACA;EACA,CAjCA;;EAkCAC,iBACA,CAnCA;;EAoCAC;IACAC;EACA,CAtCA;;EAuCAC;IACAC;MACA;MACAC;QACAC;UACAC;QADA;MADA,GAGAC,IAHA,CAGAC;QACA;;QACA;UACA;YACA;YACAC;YACA;cACAJ;gBACAxB,mBADA;gBAEAyB;cAFA;YADA,GAKAC,IALA,CAKAC;cACAC;cACA;cACA;YACA,CATA;UAWA;QACA;MAAA,CArBA;IAuBA,CA1BA;;IA2BAC;MACA;QACAL;UACAM,UADA;UAEAC,aAFA;UAGAN;QAHA;MADA,GAMAC,IANA,CAMAC;QACA;QACA;UACAH;YACAQ;UADA;QADA,GAIAN,IAJA,CAIAC;UAEA;UACA;UACA;QACA,CATA;MAUA,CAlBA;IAoBA,CAhDA;;IAmDAM;MAEA;IACA,CAtDA;;IAuDAC;MACA;QACA;UAAA1B;UAAA2B;QAAA;QACA;MACA;;MACAC;IACA,CA7DA;;IA8DAC;MACA;IACA,CAhEA;;IAiEAC;MACA;QACAF;MACA;IACA,CArEA;;IAsEAG;MACA;QACAH;MACA;IACA,CA1EA;;IA2EAI;MACA;MACA;IACA,CA9EA;;IA+EAC;MACA;MACA;QACAC,uBADA;QAEAC,sBAFA;QAGAR;MAHA,GAIAT,IAJA,CAIA;QACA;QACA,sDACAA,IADA,CACAC;UACA;YACA;YACA;UACA,CAHA,MAGA;YACA;UACA;QACA,CARA;MASA,CAfA;IAgBA,CAjGA;;;EAAA;AAvCA","names":["name","components","Progress","data","meetingList","isFullscreen","signList","signName","message","message2","message1","signInfo","allNum","signNum","rate","meeting","created","mounted","beforeDestroy","destroyed","window","methods","join","axios","params","code","then","res","console","load","pageNum","pageSize","meetingI