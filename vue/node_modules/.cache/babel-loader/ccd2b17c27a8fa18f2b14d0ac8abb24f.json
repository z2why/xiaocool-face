         pageSize: 999,\n          code: 1231\n        }\n      }).then(res => {\n        this.meetingList = res.data.records;\n        this.request.get(\"/AboutMeeting/SignNum\", {\n          params: {\n            meetingId: this.meetingList[0].meetingId\n          }\n        }).then(res => {\n          this.signNum = res.data.allcount - res.data.signcount;\n          this.allNum = res.data.allcount;\n          this.rate = Number(this.signNum / this.allNum * 100).toFixed(2);\n        });\n      });\n    },\n\n    errorHandler() {\n      return true;\n    },\n\n    click() {\n      if (!screenfull.isEnabled) {\n        this.$message({\n          message: '你的浏览器不支持全屏',\n          type: 'warning'\n        });\n        return false;\n      }\n\n      screenfull.toggle();\n    },\n\n    change() {\n      this.isFullscreen = screenfull.isFullscreen;\n    },\n\n    init() {\n      if (screenfull.isEnabled) {\n        screenfull.on('change', this.change);\n      }\n    },\n\n    destroy() {\n      if (screenfull.isEnabled) {\n        screenfull.off('change', this.change);\n      }\n    } //\n\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+GA;AACA;AACA;AACA;EACAA,eADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,eADA;MAEAC,mBAFA;MAEA;MACAC,YAHA;MAIAC,YAJA;MAKAC,YALA;MAMAC,UANA;MAOAC,WAPA;MAQAC;IARA;EAUA,CAhBA;;EAiBAC;IACAC;IACA;EAEA,CArBA;;EAsBAC;IACA;IACAC;MACAC;IACA,CAFA,EAEA,IAFA;EAGA,CA3BA;;EA4BAC;IACA;EACA,CA9BA;;EA+BAC;IACAH;EACA,CAjCA;;EAkCAI;IACAC;MACA;MACAC,gBACAC,IADA,CACAC;QACA;;QACA;UAEA;UACAV;UACA;YACAW;cACAxB,mBADA;cAEAyB;YAFA;UADA,GAKAH,IALA,CAKAC;YACAV;YACA;YACA;UACA,CATA;QAaA;MAEA,CAtBA;IAwBA,CA3BA;;IA4BAa;MACA;QACAF;UACAG,UADA;UAEAC,aAFA;UAGAH;QAHA;MADA,GAMAH,IANA,CAMAC;QACA;QACA;UACAC;YACAK;UADA;QADA,GAIAP,IAJA,CAIAC;UAEA;UACA;UACA;QACA,CATA;MAUA,CAlBA;IAoBA,CAjDA;;IAoDAO;MAEA;IACA,CAvDA;;IAwDAC;MACA;QACA;UAAAC;UAAAC;QAAA;QACA;MACA;;MACAC;IACA,CA9DA;;I