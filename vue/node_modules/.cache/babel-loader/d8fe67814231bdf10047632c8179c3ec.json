n: relative;\" >\r\n        </div>\r\n\r\n\r\n\r\n\r\n        <template slot-scope=\"content\" >\r\n          <div class=\"hxd\" align=\"center\" STYLE=\"user-select: none\" @click=\"$router.push('/userinfo')\"  @mouseenter=\"qwq=1\" @mouseleave=\"QWQ\">\r\n            <div >\r\n              <img :src=\"user.avatar\" alt=\"\" class=\"avatar1\">\r\n\r\n             <div class=\"username\"> {{ user.name }}</div>\r\n            </div>\r\n\r\n            <router-link :to=\"{path:'/'}\" @click.native=\"refresh\" style=\"text-decoration: none;\">\r\n              <el-dropdown-item divided>退出登录</el-dropdown-item>\r\n\r\n            </router-link>\r\n\r\n          </div>\r\n        </template>\r\n\r\n      </el-popover>\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Head\",\r\n  data() {\r\n    return {\r\n      index: '1',\r\n      user:{},\r\n      qwq:0,\r\n      cmditem:''\r\n    };\r\n  },\r\n  created() {\r\n    /*获取当前用户信息*/\r\n    this.user=JSON.parse(localStorage.getItem(\"user\"))\r\n\r\n    //this.getMeetingPageOfMe();\r\n  },\r\n\r\n  methods: {\r\n    getActive(){\r\n      console.log(this.$route.path)\r\n      if(this.$route.path === '/userinfo/ScheduledMeeting'||\r\n          this.$route.path === '/userinfo/sign'||\r\n          this.$route.path === '/userinfo/tabCreateMeeting' ||\r\n          this.$route.path === '/userinfo/meetinglist'||\r\n          this.$route.path === '/userinfo/applymeeting')\r\n        return '/userinfo';\r\n      else\r\n        return this.$route.path;\r\n    },\r\n    refresh(){\r\n      window.localStorage.clear()\r\n      // 状态保持清除后刷新页面\r\n      this.$message.success(\"退出成功\")\r\n      this.$router.go(0)\r\n    } ,\r\n    QoQ(){\r\n      let _this=this\r\n      setTimeout(() => {\r\n        _this.qwq=1\r\n      }, 440);\r\n    },\r\n\r\n    QWQ(){\r\n      let _this=this\r\n      setTimeout(() => {\r\n        _this.qwq=0\r\n      }, 440);\r\n      }\r\n\r\n  },\r\n\r\n};\r\n</s